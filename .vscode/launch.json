    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ct_launcher'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pixie_stitch",
                    "--package=pixie_stitch",
                ],
                "filter": {
                    "name": "pixie_stitch",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'ct_makeproject'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=ct_makeproject",
                    "--package=ct_makeproject"
                ],
                "filter": {
                    "name": "ct_makeproject",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}